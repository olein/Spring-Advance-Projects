<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
    <schema-selection match-catalog="home_master"/>
  
    <type-mapping>
        <sql-type jdbc-type="CHAR" length='1' hibernate-type="java.lang.String" />
        <sql-type jdbc-type="BLOB"  hibernate-type="binary" />
        <sql-type jdbc-type="DECIMAL"  hibernate-type="java.lang.Double" />
        <sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long"/>
    </type-mapping>
    
    <table-filter match-name="address"/>
    <table-filter match-name="contact"/>
    <table-filter match-name="task"/>
    
    <table name="address" catalog="home_master" class="com.jonak.entity.Address"/>
    <table name="contact" catalog="home_master" class="com.jonak.entity.Contact"/>
    <table name="task" catalog="home_master" class="com.jonak.entity.Task"/>
</hibernate-reverse-engineering>
